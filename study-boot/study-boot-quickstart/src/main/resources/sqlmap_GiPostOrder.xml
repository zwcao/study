<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd" >
<mapper namespace="GiPostOrder">

	<select id="queryById" parameterType="long" resultType="string">
		select post_order_code
		from wms_gi.gi_post_order
		where id = #{id}
	</select>
</mapper>